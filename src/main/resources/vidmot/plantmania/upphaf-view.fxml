        <PasswordField fx:id="fxLykilord" layoutX="338.0" layoutY="216.0"/>
        <Label layoutX="327.0" layoutY="369.0" text="Enginn aðgangur?"/>
        <Button fx:id="fxInnskraning" layoutX="312.0" layoutY="280.0" mnemonicParsing="false" onAction="#skraInn"
