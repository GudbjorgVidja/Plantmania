    <children>
        <MenuBar fx:id="fxMenuBar" layoutX="50.0" layoutY="21.0" prefHeight="29.0" prefWidth="180.0">
            <menus>
                <Menu fx:id="rodunMenu" mnemonicParsing="false" text="röðun">
                    <items>
