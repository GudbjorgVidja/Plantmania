                        <CheckMenuItem mnemonicParsing="false" text="velja allt"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <ScrollPane hbarPolicy="NEVER" layoutX="14.0" layoutY="57.0" prefHeight="414.0" prefWidth="588.0">
            <content>
